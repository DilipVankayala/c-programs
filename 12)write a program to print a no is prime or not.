#include<stdio.h>
int main()
{
    int i,x,count=0;
    printf("enter x:");
    scanf("%d",&x);
    for(i=1;i<=x;i++)
    {
        if(x%i==0)
        {
            count++;
        }
    }
    if(count==2)
    {
        printf("%d is prime",x);
    }
    else
    {
        printf("%d is not prime",x);
}
}
